#!/bin/sh

f=3 b=4
for j in f b; do
  for i in {0..7}; do
    printf -v $j$i %b "\e[${!j}${i}m"
  done
done
bld=$'\e[1m'
rst=$'\e[0m'
inv=$'\e[7m'
cat << EOF
$bld                                                                                                                                
$f3                 .,-:;//;:=,                               =+&HM####@H%;,                                =/;;/-                 
$f3             . :H@@@MM@M#H/.,+%;,                       /H###############M$,                            +:    //                
$f3          ,/X+ +M@@M@MM%=,-%HMMM@X/,                    ,@################+                            /;      /;               
$f3         -+@MM; %M@@MH+-,;XMMMM@MMMM@+-                  .H##############+                            -X        H.              
$f3        ;@M@@M- XM@X;. -+XXXXXHHH@M@M#@/.                  X############/               .//;;;:;;-,   X=        :+   .-;:=;:;%;.
$f3      ,%MM@@MH ,@%=            .---=-=:=,.                  &##########/                M-       ,=;;;#:,      ,:#;;:=,       ,@
$f3      -@#@@@MX .,              -%HX$&%%%+;                   %########/                 :%           :%.=/++++/=.$=           %=
$f3     =-./@M@M$                  .;@MMMM@MM:                   /X/;;+X/                   ,%;         %/:+/;,,/++:+/         ;+. 
$f3     X@/ -%MM/                    .+MM@@@M$                                                ,+/.    ,;@+,        ,%H;,    ,/+,   
$f3    ,@M@H: :@:                    . -X#@@@@-                   -XHHX-                         ;+;;/= @.  .H##X   -X :///+;      
$f3    ,@@@MMX, .                    /H- ;@M@M=                  ,######,                        ;+=;;;.@,  .XM@$.  =X.//;=%/.     
$f3    .H@@@@M@+,                    %MM+..%#$.  #############X  .M####M.  X#############     ,;:      :@%=        =&H:     .+%-    
$f3     /MMMM@MMH/.                  XM@MH; -;   ##############-   -//-   -##############   ,%=         %;-///==///-//         =%, 
$f3      /%+%&XHH@$=              , .H@@@@MX,    X##############%,      ,+##############X  ;+           :%-;;;;;;;;-X-           +:
$f3       .=--------.           -%H.,@@@@@MX,    -##############X        X##############-  @-      .-;;;;M-        =M/;;;-.      -X
$f3       .%MM@@@HHHXX$&$%+- .:&MMX -M@@MM%.      %############%          %############%    :;;::;;-.    %-        :+    ,-;;-;:== 
$f3         =XMMM@MM@MM#H;,-+HMM@M+ /MMMX=         %##########;            ;##########%                  ,X        H.              
$f3           =%@M@M#@&-.=&@MM@@@M; %M%=            ;#######M=              =M#######;                    ;/      %=               
$f3             ,:+$+-,/H#MMMMMMM@- -,               .+M###@,                ,@###M+.                      //    +;                
$f3                  =++%%%%+/:-.                       :XH.                  .HX:                          ,////,                 
$rst
EOF
